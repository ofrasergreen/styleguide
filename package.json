{
  "name": "telia-styleguide-poc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run gen && npm run clean && webpack --config webpack-prod.config.js && npm run build-css && npm run build-server",
    "build-css": "npm run build-css:components && npm run build-css:custom",
    "build-css:components": "postcss src/components/index.pcss -o public/css/bundle.components.css",
    "build-css:custom": "postcss src/client/css/index.pcss -o public/css/bundle.custom.css",
    "build-server": "babel ./src/server --out-dir ./dist/server --copy-files && cp -r public dist/public",
    "clean": "rm -rf dist && mkdir -p dist",
    "css:components:watch": "nodemon --ext css --watch src/components --exec \"npm run build-css:components\"",
    "css:custom:watch": "nodemon --ext css --watch src/client/css --exec \"npm run build-css:custom\"",
    "gen": "npm run generate-html-doc && npm run generate-react-doc && npm run generate-react-example-props-doc && npm run svgo",
    "dev": "npm run gen && run-p start-dev-server start-server css:components:watch css:custom:watch html:components:watch",
    "dev:fast": "run-p start-dev-server start-server css:components:watch css:custom:watch html:components:watch",
    "generate-html-doc": "babel-node ./scripts/generate-html-component-metadata.js",
    "generate-react-doc": "react-docgen ./src/components -e index.js -o component-metadata--react.json --pretty",
    "generate-react-example-props-doc": "babel-node ./scripts/generate-react-component-example-props.js",
    "heroku-postbuild": "npm run build",
    "html:components:watch": "nodemon --ext html --watch src/components --exec \"npm run generate-html-doc\"",
    "start": "node dist/server/server.js",
    "svgo": "svgo -f ./src/unoptimized-icons -o ./public/icons --config=./svgo.config.yml --multipass",
    "start-server": "babel-node src/server/server.js",
    "start-dev-server": "webpack-dev-server --config webpack-dev.config.js",
    "test": "mocha --opts test/mocha.opts --compilers js:babel-core/register test",
    "test:watch": "nodemon --watch test/ --watch src/ --ext js,jsx --exec \"npm --silent run test\"",
    "webpack:prod": "webpack --config webpack-prod.config.js"
  },
  "author": "Telia Norge",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "base64-inline-loader": "1.1.0",
    "chai": "4.1.1",
    "css-loader": "0.28.7",
    "mocha": "3.5.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.1.1",
    "postcss-calc": "6.0.0",
    "postcss-cli": "4.1.0",
    "postcss-custom-properties": "6.1.0",
    "postcss-easy-import": "2.1.0",
    "postcss-functions": "3.0.0",
    "postcss-loader": "2.0.6",
    "postcss-nested": "2.1.2",
    "proxy-middleware": "0.15.0",
    "style-loader": "0.18.2",
    "svgo": "0.7.2",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "dependencies": {
    "archiver": "^1.3.0",
    "classnames": "^2.2.5",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "pretty": "^2.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-docgen": "^2.15.0",
    "react-dom": "^15.6.1",
    "react-element-to-jsx-string": "^8.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-simpletabs": "^0.7.0",
    "redux": "^3.6.0"
  }
}
